   }
  },
  {
   "cell_type": "code",
   "source": [
    "def remove_duplicates(nums):\n",
    "    cnt = 1\n",
    "    while cnt < len(nums):\n",
    "        if nums[cnt] in nums[:cnt]:\n",
    "            nums.pop(cnt)\n",
    "        else:\n",
    "            cnt += 1\n",
    "    return cnt"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
